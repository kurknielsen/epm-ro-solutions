CREATE OR REPLACE TYPE CDI_ESCHEDULE_FORMAT_TYPE AS OBJECT
(
  CONTRACT_NUMBER NUMBER(16),
  SCHEDULE_DATE DATE,
  LOCAL_DAY DATE,
  ALT_LOCAL_HOUR NUMBER(2),
